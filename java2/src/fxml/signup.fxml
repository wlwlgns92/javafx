      </ImageView>
      <AnchorPane fx:id="signuppane" layoutX="860.0" layoutY="146.0" opacity="0.87" prefHeight="457.0" prefWidth="339.0" style="-fx-background-color: #bcabd0; -fx-background-radius: 50;">
         <children>
            <ImageView fitHeight="77.0" fitWidth="98.0" layoutX="122.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@아이콘.png" />
               </image>
            </ImageView>
            <TextField fx:id="txtid" layoutX="31.0" layoutY="122.0" prefHeight="26.0" prefWidth="258.0" promptText="User_id" style="-fx-border-color: #000000;" />
            <PasswordField fx:id="txtpassword" layoutX="31.0" layoutY="170.0" prefHeight="26.0" prefWidth="258.0" promptText="User_password" style="-fx-border-color: #000000;" />
